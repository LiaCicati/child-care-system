        <Button layoutX="35.0" layoutY="600.0" minHeight="70.0" minWidth="342.0" mnemonicParsing="false" onAction="#onEdit" style="-fx-background-radius: 37; -fx-background-color: #A094E3;" text="Edit" textFill="WHITE">
            <font>
                <Font size="16.0" />
            </font>
            <effect>
                <DropShadow color="rgba(82, 67, 154, 0.25);" />
            </effect>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <Label layoutX="148.0" layoutY="176.0" text="Hello, Lara!">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <HBox layoutX="24.0" layoutY="255.0" prefHeight="312.0" prefWidth="366.0" spacing="5.0">
            <children>
                <VBox prefHeight="166.0" prefWidth="180.0" spacing="25.0">
                    <children>
                        <Label text="First name" />
                        <Label text="Last name" />
